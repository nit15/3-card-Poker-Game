              // DCL3.setIcon(new ImageIcon("C:\\Users\\Nitya\\Desktop\\cardback.jpg"));
               msgp.setVisible(true);
               //rl.setVisible(true);
               //SwingUtilities.updateComponentTreeUI(mainp);
         }
           // PL1W.setText((Integer.parseInt(PL1W.getText())-((Integer.parseInt(PL1SPN.getValue()+""))+(Integer.parseInt(PPSET1.getValue()+""))))+"");
            //PL2W.setText((Integer.parseInt(PL1W.getText())-((Integer.parseInt(PL1SPN.getValue()+""))+(Integer.parseInt(PPSET1.getValue()+""))))+"");
        else 
         {  
       
