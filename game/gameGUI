                    {
                    rtext.append("player2 won pairpluswegar:"+"  " + 6*Integer.parseInt(PPSET2.getValue()+"")+"\n" );
                    PL2W.setText((Integer.parseInt(PL2W.getText())+( 4*Integer.parseInt(PL2SPN.getValue()+"")+6*Integer.parseInt(PPSET2.getValue()+"")))+"");
                    }   
                    else if(tc.evalHand1(cpg.hand2)==2)
                    {
                    rtext.append("player2 won pairpluswegar:"+"  " + 3*Integer.parseInt(PPSET2.getValue()+"")+"\n" );
                    PL2W.setText((Integer.parseInt(PL2W.getText())+( 4*Integer.parseInt(PL2SPN.getValue()+"")+3*Integer.parseInt(PPSET2.getValue()+"")))+"");
                    }   
                    else if(tc.evalHand1(cpg.hand2)==1)
                    {
                    rtext.append("player2 won pairpluswegar:"+"  " + Integer.parseInt(PPSET2.getValue()+"")+"\n" );
                    PL2W.setText((Integer.parseInt(PL2W.getText())+( 4*Integer.parseInt(PL2SPN.getValue()+"")+1*Integer.parseInt(PPSET2.getValue()+"")))+"");
                    }  
                    else
                    {
                    rtext.append("player2 loses pairpluswegar:" +"  "+ Integer.parseInt(PPSET2.getValue()+"")+"\n" );
                    PL2W.setText( (Integer.parseInt(PL2W.getText())+4*Integer.parseInt(PL2SPN.getValue()+""))+"");
                    }  
                    rtext.append("player1 loses playwegar:" + "  "+Integer.parseInt(PL1SPN.getValue()+"")+"\n" );
                    rtext.append("player1 loses pairpluswegar:" + "  "+Integer.parseInt(PPSET1.getValue()+"")+"\n" );
                    tc.setList();
                    rtext.append("DO YOU WANT TO CONTINUE?");
                     msgp.setVisible(true);
                     // rl.setVisible(true);
        }
        }
        else
        {
             if(tc.comparehand(cpg.hand2)==1)
            {
           // tc.setList();
            rtext.append("DEALER WON THE GAME\n");
            rtext.append("player1 lose playwegar:" +"  "+ 2*Integer.parseInt(PL1SPN.getValue()+"")+"\n" );
            rtext.append("player1 lose pairpluswegar:" +"  "+ Integer.parseInt(PPSET1.getValue()+"")+"\n" );
            rtext.append("player2 lose playwegar:" + "  "+2*Integer.parseInt(PL2SPN.getValue()+"")+"\n" );
            rtext.append("player2 loose pairpluswegar:" + "  "+Integer.parseInt(PPSET2.getValue()+"")+"\n" );
           rtext.append("DO YOU WANT TO CONTINUE?");
            msgp.setVisible(true);
           // rl.setVisible(true);
            }
             else 
                {
                    rtext.append("PLAYER2 WON THE GAME\n");
                    rtext.append("player2 won the bet :" + "  "+4*Integer.parseInt(PL2SPN.getValue()+"")+"\n" );
                    if(tc.evalHand1(cpg.hand1)==5)
                    {
                    rtext.append("player2 won pairpluswegar:"+"  " + 40*Integer.parseInt(PPSET2.getValue()+"")+"\n" );
                    PL2W.setText((Integer.parseInt(PL2W.getText())+( 4*Integer.parseInt(PL2SPN.getValue()+"")+40*Integer.parseInt(PPSET2.getValue()+"")))+"");
                    }
                    else if(tc.evalHand1(cpg.hand1)==4)
                    {
                    rtext.append("player2 won pairpluswegar:"+"  " + 30*Integer.parseInt(PPSET2.getValue()+"")+"\n" );
                    PL2W.setText((Integer.parseInt(PL2W.getText())+( 4*Integer.parseInt(PL2SPN.getValue()+"")+30*Integer.parseInt(PPSET2.getValue()+"")))+"");
                    }
                    else if(tc.evalHand1(cpg.hand2)==3)
                    {
                    rtext.append("player2 won pairpluswegar:"+"  " + 6*Integer.parseInt(PPSET2.getValue()+"")+"\n" );
                    PL2W.setText((Integer.parseInt(PL2W.getText())+( 4*Integer.parseInt(PL2SPN.getValue()+"")+6*Integer.parseInt(PPSET2.getValue()+"")))+"");
                    }   
                    else if(tc.evalHand1(cpg.hand2)==2)
                    {
                    rtext.append("player2 won pairpluswegar:"+"  " + 3*Integer.parseInt(PPSET2.getValue()+"")+"\n" );
                    PL2W.setText((Integer.parseInt(PL2W.getText())+( 4*Integer.parseInt(PL2SPN.getValue()+"")+3*Integer.parseInt(PPSET2.getValue()+"")))+"");
                    }   
                    else if(tc.evalHand1(cpg.hand2)==1)
                    {
                    rtext.append("player2 won pairpluswegar:"+"  " + Integer.parseInt(PPSET2.getValue()+"")+"\n" );
                    PL2W.setText((Integer.parseInt(PL2W.getText())+( 4*Integer.parseInt(PL2SPN.getValue()+"")+1*Integer.parseInt(PPSET2.getValue()+"")))+"");
                    }  
                    else
                    {
                    rtext.append("player2 loses pairpluswegar:" +"  "+ Integer.parseInt(PPSET2.getValue()+"")+"\n" );
                    PL2W.setText( (Integer.parseInt(PL2W.getText())+4*Integer.parseInt(PL2SPN.getValue()+""))+"");
                    }  
                    rtext.append("player1 loses playwegar:" + "  "+Integer.parseInt(PL1SPN.getValue()+"")+"\n" );
                    rtext.append("player1 loses pairpluswegar:" + "  "+Integer.parseInt(PPSET1.getValue()+"")+"\n" );
                  
                   // tc.setList();
                    rtext.append("DO YOU WANT TO CONTINUE?");
                    msgp.setVisible(true);
                    // rl.setVisible(true);
        
                }
            
        }
        
    }
    else
    {
        JOptionPane.showMessageDialog(mainp, "IT'S PLAYER1'S TURN");
    }
// TODO add your handling code here:
    }                                      

