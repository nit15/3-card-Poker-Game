    private void FLBTN2ActionPerformed(java.awt.event.ActionEvent evt) {                                       
      if(t==true)
      {PL2NAME.setEnabled(false);
        CL4.setEnabled(false);
        CL5.setEnabled(false);
        CL6.setEnabled(false);
        PLBTN2.setEnabled(false);
        PL2SPN.setEnabled(false);
        DCL1.setIcon(new ImageIcon("C:\\Users\\Nitya\\Desktop\\cards\\" + dc1 +".jpg"));
        DCL2.setIcon(new ImageIcon("C:\\Users\\Nitya\\Desktop\\cards\\" + dc2 +".jpg"));
        DCL3.setIcon(new ImageIcon("C:\\Users\\Nitya\\Desktop\\cards\\" +dc3 +".jpg"));
        CL1.setIcon(new ImageIcon("C:\\Users\\Nitya\\Desktop\\cards\\" +c1 +".jpg"));
        CL2.setIcon(new ImageIcon("C:\\Users\\Nitya\\Desktop\\cards\\" + c2 +".jpg"));
        CL3.setIcon(new ImageIcon("C:\\Users\\Nitya\\Desktop\\cards\\" + c3 +".jpg"));
        System.out.println(tc.evalHand1(cpg.hand1));
        System.out.println(tc.evalHand1(cpg.hand2));
        tc.compareplayer();
        tc.comparehand(cpg.hand1);
        if(c==1)
        {
            //tc.setList();
            rtext.append("DEALER WON THE GAME\n");
            rtext.append("player1 lose playwegar:" +"  "+ Integer.parseInt(PL1SPN.getValue()+"")+"\n" );
            rtext.append("player1 lose pairpluswegar:" +"  "+ Integer.parseInt(PPSET1.getValue()+"")+"\n" );
            rtext.append("player2 lose playwegar:" + "  "+Integer.parseInt(PL2SPN.getValue()+"")+"\n" );
            rtext.append("player2 loose pairpluswegar:" + "  "+Integer.parseInt(PPSET2.getValue()+"")+"\n" );
            rtext.append("DO YOU WANT TO CONTINUE?");
            msgp.setVisible(true);
           // rl.setVisible(true);
        }
        
        else if(tc.comparehand(cpg.hand1)==1)
        {  
            rtext.append("DEALER WON THE GAME\n");
            rtext.append("player1 lose playwegar:" + "  "+2*Integer.parseInt(PL1SPN.getValue()+"")+"\n" );
            rtext.append("player1 lose pairpluswegar:" + "  "+Integer.parseInt(PPSET1.getValue()+"")+"\n" );
            rtext.append("player2 lose playwegar:" + "  "+Integer.parseInt(PL2SPN.getValue()+"")+"\n" );
            rtext.append("player2 loose pairpluswegar:" + "  "+Integer.parseInt(PPSET2.getValue()+"")+"\n\n" );
            rtext.append("DO YOU WANT TO CONTINUE?");
            msgp.setVisible(true);
           //  rl.setVisible(true);
        }
        else 
                {
                   
                    rtext.append("PLAYER1 WON THE GAME\n");
                    rtext.append("player1 won the bet :" + "  "+4*Integer.parseInt(PL1SPN.getValue()+"")+"\n" );
                    if(tc.evalHand1(cpg.hand1)==5)
                    {
                    rtext.append("player1 won pairpluswegar:"+"  " + 40*Integer.parseInt(PPSET1.getValue()+"")+"\n" );
                    PL1W.setText((Integer.parseInt(PL1W.getText())+( 4*Integer.parseInt(PL1SPN.getValue()+"")+40*Integer.parseInt(PPSET1.getValue()+"")))+"");
                    }
                    else if(tc.evalHand1(cpg.hand1)==4)
                    {
                    rtext.append("player1 won pairpluswegar:"+"  " + 30*Integer.parseInt(PPSET1.getValue()+"")+"\n" );
                    PL1W.setText((Integer.parseInt(PL1W.getText())+( 4*Integer.parseInt(PL1SPN.getValue()+"")+30*Integer.parseInt(PPSET1.getValue()+"")))+"");
                    }
                    else if(tc.evalHand1(cpg.hand1)==3)
                    {
                    rtext.append("player1 won pairpluswegar:"+"  " + 6*Integer.parseInt(PPSET1.getValue()+"")+"\n" );
                    PL1W.setText((Integer.parseInt(PL1W.getText())+( 4*Integer.parseInt(PL1SPN.getValue()+"")+6*Integer.parseInt(PPSET1.getValue()+"")))+"");
                    }   
                    else if(tc.evalHand1(cpg.hand1)==2)
                    {
                    rtext.append("player1 won pairpluswegar:"+"  " + 3*Integer.parseInt(PPSET1.getValue()+"")+"\n" );
                    PL1W.setText((Integer.parseInt(PL1W.getText())+( 4*Integer.parseInt(PL1SPN.getValue()+"")+3*Integer.parseInt(PPSET1.getValue()+"")))+"");
                    }   
                    else if(tc.evalHand1(cpg.hand1)==1)
                    {
                    rtext.append("player1 won pairpluswegar:"+"  " + Integer.parseInt(PPSET1.getValue()+"")+"\n" );
                    PL1W.setText((Integer.parseInt(PL1W.getText())+( 4*Integer.parseInt(PL1SPN.getValue()+"")+Integer.parseInt(PPSET1.getValue()+"")))+"");
                    }  
                    else
                    {
                    rtext.append("player1 loses pairpluswegar:" +"  "+ Integer.parseInt(PPSET1.getValue()+"")+"\n" );
                    PL1W.setText((Integer.parseInt(PL1W.getText())+4*Integer.parseInt(PL1SPN.getValue()+""))+"");
                    }  
                    rtext.append("player2 loses playwegar:" + "  "+Integer.parseInt(PL2SPN.getValue()+"")+"\n" );
                    rtext.append("player2 loses pairpluswegar:" + "  "+Integer.parseInt(PPSET2.getValue()+"")+"\n" );
                    
                    tc.setList();
                    rtext.append("DO YOU WANT TO CONTINUE?");
                    msgp.setVisible(true);
                   //  rl.setVisible(true);
                }
      }
      else
      {
          JOptionPane.showMessageDialog(mainp, "IT'S PLAYER1'S TURN");
      }// TODO add your handling code here:
    }                                      

