    private void suffleActionPerformed(java.awt.event.ActionEvent evt) {                                       
        d.getdeck();
        
        c1=d.cardlist.get(random.nextInt(d.cardlist.size()))+"";
        d.cardlist.remove(c1);
        c2=d.cardlist.get(random.nextInt(d.cardlist.size()))+"";
        d.cardlist.remove(c2);
        c3=d.cardlist.get(random.nextInt(d.cardlist.size()))+"";
        d.cardlist.remove(c3);
        c4=d.cardlist.get(random.nextInt(d.cardlist.size()))+"";
        d.cardlist.remove(c4);
        c5=d.cardlist.get(random.nextInt(d.cardlist.size()))+"";
        d.cardlist.remove(c5);
        c6=d.cardlist.get(random.nextInt(d.cardlist.size()))+"";
        d.cardlist.remove(c6);
        //System.out.println(d.cardlist);
        dc1=d.cardlist.get(random.nextInt(d.cardlist.size()))+"";
        d.cardlist.remove(dc1);
        dc2=d.cardlist.get(random.nextInt(d.cardlist.size()))+"";
        d.cardlist.remove(dc2);
        dc3=d.cardlist.get(random.nextInt(d.cardlist.size()))+"";
        d.cardlist.remove(dc3);
        d.cardlist.removeAll(d.cardlist);
       // System.out.println(d.cardlist);
        dl.getdealercard(dc1);
        dl.getdealercard(dc2);
        dl.getdealercard(dc3);
        dl.checkdealhand();
        System.out.println(dl.checkdealhand());
       // System.out.println(tc.checkhighcard(cpg.dealhand));
        
        if(dl.checkdealhand())
         {    
              //DCL1.setText(d.cardlist.get(random.nextInt(d.cardlist.size()))+"");
               DCL1.setIcon(new ImageIcon("C:\\Users\\Nitya\\Desktop\\cards\\" + dc1+".jpg"));
               DCL2.setIcon(new ImageIcon("C:\\Users\\Nitya\\Desktop\\cards\\" + dc2 +".jpg"));
               DCL3.setIcon(new ImageIcon("C:\\Users\\Nitya\\Desktop\\cards\\" + dc3 +".jpg"));
               rtext.append("DEALER ONLY PLAY WITH ATLEAST\n QUEEN AND A HIGH CARD\n\n");
